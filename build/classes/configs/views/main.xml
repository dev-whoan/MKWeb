<?xml version="1.0" encoding="UTF-8"?>

<views>
	<Control name="" debug="error" dir="/views/root" dir_key="/" page="main.jsp" >
		<!-- 
		<PageParams name="PAGE_STATIC">
			@set(wrap_post_id = '%')
		</PageParams>
		 -->
		<Service id="selectName" type="sql">
			<Sql obj="list" method="post" />
			<Parameter name="param" />
			<Value>
				@set(param.user_name = '%')
				@set(param.user_info = '%')
			</Value>
		</Service>
		
		<Service id="insertUser" type="sql">
			<Sql method="post" />
			<Parameter name="ins" />
			<Value>
				@set(ins.qweqwe = '%')
				@set(ins.qqwwee = '%')
			</Value>
		</Service>
		
		<Service id="updateUser" type="sql">
			<Sql method="post" />
			<Parameter name="upd" />
			<Value>
				@set(upd.user_name = '%')
				@set(upd.user_info = '%')
				@set(upd.user_SEQ = '%')
			</Value>
		</Service>
		
		<Service id="deleteUser" type="sql">
			<Sql method="post" />
			<Parameter name="del" />
			<Value>
				@set(upd.user_SEQ = '%')
			</Value>
		</Service>
	</Control>
</views>