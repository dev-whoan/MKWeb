<?xml version="1.0" encoding="UTF-8"?>

<views>
	<Control name="" debug="error" dir="/views/root" dir_key="/" page="main.jsp" >
		<!-- 
		<PageParams name="PAGE_STATIC">
			@set(wrap_post_id = '%')
		</PageParams>
		 -->
		<Service type="sql">
			<Sql id="selectName" obj="list" result="alpha" method="post" />
			<Parameter name="param" />
			<Value>
				@set(param.user_name = '%')
				@set(param.user_info = '%')
			</Value>
		</Service>
		
		<Service type="sql">
			<Sql id="insertUser" method="post" />
			<Parameter name="ins" />
			<Value>
				@set(ins.qweqwe = '%')
				@set(ins.qqwwee = '%')
			</Value>
		</Service>
		
		<Service type="sql">
			<Sql id="updateUser" method="post" />
			<Parameter name="upd" />
			<Value>
				@set(upd.user_name = '%')
				@set(upd.user_info = '%')
				@set(upd.user_SEQ = '%')
			</Value>
		</Service>
		
		<Service type="sql">
			<Sql id="deleteUser" method="post" />
			<Parameter name="del" />
			<Value>
				@set(upd.user_SEQ = '%')
			</Value>
		</Service>
	</Control>
</views>